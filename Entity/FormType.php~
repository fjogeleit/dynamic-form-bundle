<?php

namespace DynamicFormBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Table
 * @ORM\Entity
 *
 * @package DynamicFormBundle\Entity
 */
class FormType
{
    /**
     * @var int
     *
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(type="string")
     */
    private $name;

    /**
     * Set id
     *
     * @param integer $id
     *
     * @return FormType
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set formType
     *
     * @param string $name
     *
     * @return FormType
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get formType
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
}
